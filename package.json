    "next": "^15.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "recharts": "^3.1.0",
    "rollbar": "^2.26.4",
    "swr": "^2.3.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.0.3",
    "zod": "^3.25.76"
